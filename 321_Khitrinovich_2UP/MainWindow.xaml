<Window x:Class="_321_Khitrinovich_2UP.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_321_Khitrinovich_2UP"
        mc:Ignorable="d"
        Title="Приложение Мастер Пол" Height="850" Width="1200" Closing="CloseWindow">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="45*"></RowDefinition>
            <RowDefinition Height="360*"></RowDefinition>
            <RowDefinition Height="29*"></RowDefinition>
        </Grid.RowDefinitions>
        <Frame Source="AllPages/Menu.xaml" Name="NavigationService" BorderThickness="2"  Navigated="MainFrame_OnNavigated" NavigationUIVisibility="Hidden" Grid.Row="1" Margin="0,15,0,14"/>
        <Button ToolTip="Кнопка назад" Margin="10,0,0,10" Content="Назад" x:Name="BackButton" Click="BackButtonClick" Height="49" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="251"></Button>
    </Grid>
</Window>
