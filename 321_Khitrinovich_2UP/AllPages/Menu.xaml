<Page x:Class="_321_Khitrinovich_2UP.AllPages.Menu"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:_321_Khitrinovich_2UP.AllPages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Меню">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="73*"></RowDefinition>
            <RowDefinition Height="346*"></RowDefinition>
            <RowDefinition Height="31*"></RowDefinition>
        </Grid.RowDefinitions>

        <Button x:Name="WatchPartnersButton" Content="Просмотр списка партнеров" Grid.Row="1" Height="56" VerticalAlignment="Top" HorizontalAlignment="Center" Width="242" Click="WatchPartnersButtonClick" Margin="0,138,0,0"/>
        <Button x:Name="WatchHistoryButton" Content="Просмотр истории реализации продукции партнером" Margin="0,209,0,0" Grid.Row="1" Height="63" HorizontalAlignment="Center" Width="312" Click="WatchHistoryButtonClick" VerticalAlignment="Top"/>
        <Image Margin="0,10,0,0" Source="/Мастер пол.png" Stretch="Fill" RenderTransformOrigin="0.397,0.39" Grid.Row="1" Height="104" VerticalAlignment="Top" HorizontalAlignment="Center" Width="104">
        </Image>
    </Grid>
</Page>
